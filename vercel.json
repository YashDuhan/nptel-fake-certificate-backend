{
    "version": 2,
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/certificate",
        "dest": "app/main.py"
      },
      {
        "src": "/health",
        "dest": "app/main.py"
      },
      {
        "src": "/assets/(.*)",
        "dest": "app/main.py"
      }
    ]
  }
  